%data "35_Variablenprodukte-data.A.cdat"


# Produkt mit einer Binaervariable
var:
	bx: binary;
	by: real[YU..YO];
objective:
	#p_bin$4_$: bx*by;

# Produkt mit einer ganzzahligen Variable
var:
	ix: integer[XU..XO];
	iy: real[YU..YO];
objective:
	p_int$4_$: ix*iy;

# Produkt aus stetigen Variable
var:
	rx: real[XU..XO];
	ry: real[YU..YO];
objective:
	#p_real$4_$: rx*ry;


 
