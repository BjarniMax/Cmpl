# Beispiel f√ºr Produkt-Linearisierung
const:
	XU := 0; XO := 10;
	YU := 5; YO := 25;

# Produkt mit einer Binaervariable
var:
	bx: binary;
	by: real[YU..YO];
objective:
	#p_bin: bx*by;

# Produkt mit einer ganzzahligen Variable
var:
	ix: integer[XU..XO];
	iy: real[YU..YO];
objective:
	#p_int: ix*iy;

# Produkt aus stetigen Variable
var:
	rx: real[XU..XO];
	ry: real[YU..YO];
objective:
	p_real: rx*ry;


 
